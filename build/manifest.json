{
  "manifest_version": 2,
  "name": "Ncueeclass Downloadpdf-extension",
  "version": "21.12.18",
  "description": "Create a pdf download link, if theres' no link",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["*://ncueeclass.ncu.edu.tw/media/doc/*"],
      "js": ["contentScript.js"]
    }
  ]
}
